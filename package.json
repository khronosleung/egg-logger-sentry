{
  "name": "egg-logger-sentry",
  "version": "1.0.0-beta.1",
  "description": "Logger transport for Sentry",
  "eggPlugin": {
    "name": "loggerSentry"
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "sentry",
    "sentry-node",
    "sentry-egg"
  ],
  "dependencies": {
    "@sentry/node": "^5.14.2",
    "egg-logger": "^2.4.1"
  },
  "devDependencies": {
    "autod": "^3.1.0",
    "autod-egg": "^1.1.0",
    "egg": "^2.26.0",
    "egg-bin": "^4.14.1",
    "egg-ci": "^1.13.1",
    "egg-mock": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-egg": "^8.0.1",
    "ko-sleep": "^1.0.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
